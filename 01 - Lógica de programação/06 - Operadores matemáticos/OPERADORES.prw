#include 'protheus.ch'
#include 'parmtype.ch'

user function OPERADORES()

    // Declaração de variáveis
    Local nNum1 := 10
    Local nNum2 := 20

    // Somando
    Alert(nNum1 + nNum2)

    //Subtração
    Alert(nNum2 - nNum1)

    //Multiplicação
    Alert(nNum1 * nNum2)

    //Divisão
    Alert(nNum2 / nNum1)

    //Capturando resto de divisão
    Alert(nNum2 % nNum1)

RETURN
